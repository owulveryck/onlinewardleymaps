{
    "name": "owm",
    "version": "1.0.0",
    "description": "Embeddable Wardley Map renderer - like mermaid.js for Wardley Maps",
    "main": "dist/owm.umd.js",
    "module": "dist/owm.esm.mjs",
    "types": "dist/index.d.ts",
    "files": [
        "dist"
    ],
    "scripts": {
        "build": "rollup -c rollup.config.mjs",
        "build:watch": "rollup -c rollup.config.mjs -w",
        "serve": "npx http-server examples -p 8080"
    },
    "keywords": [
        "wardley",
        "wardley-maps",
        "diagram",
        "svg",
        "visualization"
    ],
    "author": "OnlineWardleyMaps",
    "license": "MIT",
    "devDependencies": {
        "@rollup/plugin-commonjs": "^28.0.3",
        "@rollup/plugin-node-resolve": "^16.0.1",
        "@rollup/plugin-replace": "^6.0.2",
        "@rollup/plugin-terser": "^0.4.4",
        "@rollup/plugin-typescript": "^12.1.2",
        "rollup": "^4.55.1",
        "tslib": "^2.8.1",
        "typescript": "^5.9.3"
    },
    "dependencies": {
        "react": "^19.2.3",
        "react-dom": "^19.2.3"
    }
}
